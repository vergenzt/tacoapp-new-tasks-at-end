{
  "manifest_version": 2,
  "name": "TacoApp Tweaks",
  "description": "Custom tweaks to TacoApp's frontend.",
  "version": "0.2",
  "content_scripts": [
    {
      "js": ["index.js"],
      "matches": [
        "https://tacoapp.com/tasks"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    "move-new-tasks-to-end-of-queue.js"
  ]
}
